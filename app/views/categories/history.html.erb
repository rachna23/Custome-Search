<% if @images.present? %>
  <table id="runner" class="display" cellpadding="0" cellspacing="0" border="0" class="table table-bordered table-striped">    
      <thead>
        <tr>
          <th> <%= "Date" %></th>
          <th> <%=  "Time" %> </th>
          <th> <%= "Noun" %> </th>
          <th> <%= "Color" %> </th>
          <th> <%= "Http response ms" %> </th>
        </tr>
      </thead>

      <tbody>
        <% @images.each do |image| %>
       <tr>
        <td> <%= image.DateTime ? image.DateTime : "NA" %> </td>
        <td> <%= image.DateTime ? image.DateTime : "NA" %> </td>
        <td> <%= image.image_file_name ? image.image_file_name : "NA" %> </td>
        <td> <%= image.color ? image.color : "NA" %> </td>
        <td> <%= image.response ? image.response : "NA" %> </td>
      </tr>
        <% end %>
      </tbody>
    </table>
<% end %>

<script>
$(document).ready(function() {
    $('#runner').dataTable( {
      "order": [[ 4, 'asc' ]]
    } );
  } );
</script>
