<% if @histories.present? %>
  <table id="runner" class="display" cellpadding="0" cellspacing="0" border="0" class="table table-bordered table-striped">    
      <thead>
        <tr>
          <th> <%= "Date" %></th>
          <th> <%=  "Time" %> </th>
          <th> <%= "Noun" %> </th>
          <th> <%= "Color" %> </th>
          <th> <%= "Http response ms" %> </th>
          <th> <%= "Search again" %> </th>
        </tr>
      </thead>

      <tbody>
        <% @histories.each do |history| %>
       <tr>
        <td> <%= date(history) %> </td>
        <td> <%= time(history) %> </td>
        <td> <%= history.noun %> </td>
        <td> <%= history.color ? history.color : "NA" %> </td>
        <td> <%= history.response ? history.response : "NA" %> </td>
        <td> <%= link_to "Search", search_again_categories_path(noun:history.noun,color:history.color),remote:true %> </td>
      </tr>
        <% end %>
      </tbody>
    </table>
<% end %>
<div id="div3"> </div> 
<script>
$(document).ready(function() {
    $('#runner').dataTable( {
      "order": [[ 4, 'asc' ]]
    } );
  } );
</script>
